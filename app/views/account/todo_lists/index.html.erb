<div class="container todo_lists">
  <div class="user-nav-bg">
    <table>
      <tbody>
        <tr>
          <td>pic</td>
          <td>快速计划</td>
        </tr>
      </tbody>
    </table>
  </div>

  <div class="todo_list_form">
    <div id="form">
      <%= render 'account/todo_lists/form' %>
    </div>
  </div>
  <% @todo_lists.each do |todo_list| %>
    <div class="index_row clearfix">
      <h2 class="todo_list_title"><%= link_to todo_list.title, account_todo_list_path(todo_list) %></h2>
      <p class="todo_list_sub_title"><%= todo_list.description%></p>
    </div>
  <% end %>
</div>
