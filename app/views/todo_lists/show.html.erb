
<div class="todo_list_title">
  <%= @todo_list.title %>
  <%= @todo_list.description %>
</div>


<div class="todo_item">
  <%= render @todo_list.todo_items %>
  <div class="forms">
    <%= render 'todo_items/form' %>
  </div>
</div>

<div class="links">
  <%= link_to "Edit", edit_todo_list_path(@todo_list) %>|
  <%= link_to "Delete", todo_list_path(@todo_list), method: :delete %>|
  <%= link_to "Back", todo_lists_path %>
</div>
